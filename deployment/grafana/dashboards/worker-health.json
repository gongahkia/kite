{
  "dashboard": {
    "title": "Kite Worker Health",
    "tags": ["kite", "worker", "jobs"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Worker Utilization",
        "type": "gauge",
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "(worker_active_count / worker_total_count) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 20, "color": "yellow"},
                {"value": 50, "color": "green"},
                {"value": 90, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Active Workers",
        "type": "stat",
        "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "worker_active_count",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Total Workers",
        "type": "stat",
        "gridPos": {"x": 6, "y": 4, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "worker_total_count",
            "refId": "A"
          }
        ]
      },
      {
        "id": 4,
        "title": "Queue Size",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "worker_queue_size",
            "legendFormat": "Queue Size",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Jobs in Queue"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [5000], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "10m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "name": "High Queue Backlog"
        }
      },
      {
        "id": 5,
        "title": "Job Processing Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(worker_jobs_processed_total{status=\"success\"}[5m]))",
            "legendFormat": "Success",
            "refId": "A"
          },
          {
            "expr": "sum(rate(worker_jobs_processed_total{status=\"failed\"}[5m]))",
            "legendFormat": "Failed",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Jobs/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Job Success Rate (%)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "(sum(rate(worker_jobs_processed_total{status=\"success\"}[5m])) / sum(rate(worker_jobs_processed_total[5m]))) * 100",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "min": 0, "max": 100},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Job Processing by Type",
        "type": "graph",
        "gridPos": {"x": 0, "y": 16, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(worker_jobs_processed_total[5m])) by (job_type)",
            "legendFormat": "{{job_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops"},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "title": "Job Duration (P50, P95, P99)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 24, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(worker_job_duration_seconds_bucket[5m])) by (le, job_type))",
            "legendFormat": "p50 - {{job_type}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(worker_job_duration_seconds_bucket[5m])) by (le, job_type))",
            "legendFormat": "p95 - {{job_type}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(worker_job_duration_seconds_bucket[5m])) by (le, job_type))",
            "legendFormat": "p99 - {{job_type}}",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "title": "Dead Letter Queue Size",
        "type": "stat",
        "gridPos": {"x": 0, "y": 32, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "queue_dlq_size",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 100, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Job Failures by Type",
        "type": "table",
        "gridPos": {"x": 12, "y": 32, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(worker_jobs_processed_total{status=\"failed\"}[1h])) by (job_type)",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ]
      }
    ]
  }
}
